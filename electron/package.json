{
  "name": "FoE_bot",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron . --disable-d3d11",

    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/Favicon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Forge of Empires Bot\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "devDependencies": {
    "electron": "^5.0.2",
    "electron-packager": "^14.0.1",
    "electron-reload": "^1.4.0",
    "python-shell": "^1.0.7"
  },
  "dependencies": {
  }
}
